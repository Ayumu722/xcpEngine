derivative() {
   eval ${1}[${cxt}]=${outdir}/${2}.nii.gz
   export derivatives_floating=$(echo ${derivatives_floating}|${JQ_PATH} '.'${1}' += {Map:"'"${outdir}"'/'"${2}"'.nii.gz", Space:"'"${space}"'", ProvenanceName:"'"${mod_name_short}"'", ProvenanceIndex:"'"${cxt}"'", Statistic:"null", Type:"map"}')
}
