atlas_parse() {
   a_name=$(return_field ${1} 2 '#')
   a_name=$(eval echo ${a_name})
   local a2=$(return_field ${1} 3 '#')
   if [[ -e ${a2} ]]
      then
      a_path=${a2}
      a_space=$(return_field ${1} 4 '#')
      a_path=$(eval echo ${a_path})
      a_path=$(ls -d1 ${a_path} 2>/dev/null)
      if is_image ${a2}
         then
         a_type=image
      else
         a_type=sclib
      fi
   else
      a_coor=${a2}
      a_radius=$(return_field ${1} 4 '#')
      a_type=sphere
   fi
}
