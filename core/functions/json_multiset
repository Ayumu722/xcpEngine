json_multiset() {
   local i asgt key val
   local multiset=()
   local json=${1}
   local object=${2}
   shift 2
   for i in ${@}
      do
      asgt=${1}
      key=$(strslice ${asgt} 1 ':')
      val=$(strslice ${asgt} 2 ':')
      multiset=( "${multiset[@]}" '"'"${key}"'":"'"${val}"'"' )
   done
   multiset=$(join_by ', ' "${multiset[@]}")
   eval ${json}=$(echo ${!json}|$JQ_PATH '."'"${object}"'" += { '"${multiset}"' }')
}
