apply_exec() {
   local deriv derivative derivatives echo_var final_cmd match output output_long property properties value values process_generic process_specific d_map d_mod d_mod_idx d_stat d_name d_space d_type
   declare -A properties=(
      ["Map"]="d_map"
      ["ProvenanceIndex"]="d_mod_idx"
      ["ProvenanceName"]="d_mod_name"
      ["Space"]="d_space"
      ["Statistic"]="d_stat"
      ["Type"]="d_type"
   )
   if contains ${1} ":"
      then
      property=$(strslice  "${1}" 1 :)
      values=$(  strslice  "${1}" 2 :)
      values=( ${values//|/ } )
   else
      property=Type
      values=( ${1//|/ }     )
   fi
   output=${2}
   if [[ ${output:0:1} == '/' ]]
      then
      output_long=${output}.nii.gz
   else
      output_long=${outdir}/${output}.nii.gz
   fi
   shift 2
   if contains ${1} 'ECHO:'
      then
      echo_var=${1//ECHO:/}
      shift
   fi
   process_generic=${@}
   load_derivatives
   for derivative in ${derivatives[@]}
      do
      match=0
      derivative_parse ${derivative}
      for value in "${values[@]}"
         do
         [[ ${value} == all ]]                        && match=1 && break
         contains ${!properties[$property]} ${value}  && match=1 && break
      done
      if (( ${match} == 1 ))
         then
         [[ -n ${echo_var} ]] && echo "Â· [${!echo_var}]"
         process_specific=${process_generic//%INPUT/${d_map}}
         process_specific=${process_specific//%OUTPUT/${output_long}}
         process_specific=${process_specific//%NAME/${d_name}}
         exec_${process_specific}
         if is_image ${output_long}
            then
            output_long=${output_long//.nii.gz/}
            derivative        ${d_name}            ${output_long//%NAME/${d_name}}
            derivative_config ${d_name} Type       ${d_type}
            derivative_config ${d_name} Space      ${d_space}
            derivative_config ${d_name} Statistic  ${d_stat}
         fi
      fi
   done
}
